{
    "manifest_version": 2,
    "name": "このはモードPlus!",
    "version": "1.0",
    "page_action": {
	"default_title": "このはモードPlus!",
	"default_popup": "popup.html",
	"default_icon": "icon.png"
    },
    "content_scripts": [
	{
	    "matches": ["https://*.conoha.jp/*"],
	    "js": ["content.js"]
	}
    ],
    "background": {
	"scripts": ["background.js"],
	    "persistent": false
    },
    "permissions": [
	"tabs", "https://manage.conoha.jp/*"
    ]
}
