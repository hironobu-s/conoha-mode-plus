{
    "manifest_version": 2,
    "name": "このはモードPlus!",
    "version": "1.0",
    "page_action": {
	"default_title": "このはモードPlus!",
	"default_popup": "popup.html",
	"default_icon": "icon.png"
    },
    "content_scripts": [
	{
	    "matches": ["https://*.conoha.jp/*"],
	    "js": ["content.js"],
	    "run_at": "document_start"
	}
    ],
    "background": {
	"scripts": ["background.js"],
	"persistent": false
    }
}
